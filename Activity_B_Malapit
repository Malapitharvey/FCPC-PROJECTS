#ACTIVITY B

print(1)
for x in range(8):
    print(2**x , end="\t")
print()

print(2)
for z in range(1,13):
    if z % 4 == 0:
        print("S", end = "\t")
    else:
        print(z , end = "\t")
print()


print(3)
for y in range(1,9):
    if y % 9 == 1:
        print(y, end = "\t")
    else:
        print("E" , end = "\t")


